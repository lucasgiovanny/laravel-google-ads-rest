{
    "name": "lucasgiovanny/laravel-google-ads-rest",
    "description": "Use Laravel Google Ads REST API easy",
    "keywords": [
        "lucasgiovanny",
        "laravel",
        "laravel-google-ads",
        "google-ads"
    ],
    "homepage": "https://github.com/lucasgiovanny/laravel-google-ads-rest",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Giovanny",
            "email": "lucasgiovanny@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.2",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "LucasGiovanny\\LaravelGoogleAds\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LucasGiovanny\\LaravelGoogleAds\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LucasGiovanny\\LaravelGoogleAds\\LaravelGoogleAdsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
